{
  "name": "Airtime2Cash",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "yarn nodemon ./bin/www",
    "start": "CI= NODE_ENV=production node ./bin/www",
    "dev:start": "npx npm-run-all dev:build dev:run",
    "dev:run": "NODE_ENV=development yarn nodemon",
    "dev:build": "yarn tsc",
    "dev:build-watch": "yarn tsc -w",
    "eslint": "./node_modules/eslint/bin/eslint.js --ext .ts ./src",
    "lint-fix": "yarn eslint -- --fix",
    "lint": "yarn eslint && yarn dev:build",
    "migrate": "NODE_ENV=test npx sequelize-cli db:migrate",
    "migrate:reset": "NODE_ENV=test npx sequelize-cli db:migrate:undo:all",
    "test": "NODE_ENV=test yarn run migrate:reset && NODE_ENV=test jest --detectOpenHandles --colors --verbose --ci",
    "coverage": "yarn test -- --coverage --watchAll=false "
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "~1.32.0",
    "cookie-parser": "~1.4.4",
    "cors": "~2.8.5",
    "debug": "~4.3.2",
    "dotenv": "^10.0.0",
    "express": "~4.17.1",
    "http-errors": "~1.8.0",
    "http-status": "~1.5.3",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "~3.1.2",
    "moment": "~2.29.4",
    "morgan": "~1.10.0",
    "node-mocks-http": "~1.11.0",
    "nodemailer": "~6.7.8",
    "nodemon": "^2.0.12",
    "sequelize": "~6.21.6",
    "sqlite3": "~5.0.11",
    "supertest": "^6.1.6",
    "uuid": "~9.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "~1.4.2",
    "@types/cors": "~2.8.12",
    "@types/debug": "~4.1.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "~4.17.13",
    "@types/http-errors": "~1.8.1",
    "@types/http-status": "~1.1.2",
    "@types/jest": "^27.0.1",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/morgan": "~1.9.3",
    "@types/node": "14",
    "@types/nodemailer": "~6.4.6",
    "@types/sqlite3": "~3.1.8",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "~8.3.4",
    "@typescript-eslint/eslint-plugin": "~4.29.1",
    "@typescript-eslint/parser": "~4.29.1",
    "cross-env": "~7.0.3",
    "eslint": "~7.32.0",
    "eslint-config-prettier": "~8.3.0",
    "husky": "~8.0.1",
    "jest": "^27.1.1",
    "prettier": "~2.3.2",
    "sequelize-cli": "~6.4.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "~4.3.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test"
    }
  }
}
